{
  "name": "auro-wallet",
  "version": "2.4.5",
  "private": true,
  "dependencies": {
    "@ledgerhq/hw-transport-webhid": "6.30.9",
    "@ledgerhq/devices": "8.7.0",
    "@reduxjs/toolkit": "2.11.1",
    "@aurowallet/mina-provider": "1.0.12",
    "axios": "1.13.2",
    "axios-retry": "4.5.0",
    "bignumber.js": "9.3.0",
    "@scure/bip32": "1.7.0",
    "@scure/bip39": "1.6.0",
    "@scure/base": "^2.0.0",
    "classnames": "2.5.1",
    "webextension-polyfill": "0.12.0",
    "i18next": "25.7.3",
    "libsodium-wrappers-sumo": "0.7.15",
    "mina-signer": "3.1.0",
    "@zondax/ledger-mina-js": "0.1.1",
    "zustand":"5.0.9",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-i18next": "16.5.0",
    "react-idle-timer": "5.7.2",
    "react-redux": "9.2.0",
    "react-router-dom": "7.11.0",
    "react-circular-progressbar": "2.2.0",
    "buffer": "6.0.3",
    "uuid": "11.1.0",
    "qrcode.react": "4.2.0",
    "styled-components": "6.1.19",
    "node-forge": "1.3.3",
    "o1js": "2.4.0",
    "serialize-error": "12.0.0",
    "mina-attestations": "0.5.0",
    "@noble/hashes": "1.7.1",
    "immer": "10.1.1"
  },
  "scripts": {
    "clean": "rm -rf dist dist-chrome dist-firefox zip",
    "build:chrome": "rm -rf dist dist-chrome && webpack --mode production --env BROWSER=chrome && mv dist dist-chrome",
    "build:firefox": "rm -rf dist dist-firefox && webpack --mode production --env BROWSER=firefox && mv dist dist-firefox",
    "build": "npm run clean && npm run build:chrome && npm run build:firefox",
    "postbuild": "node scripts/post-build.js",
    "predev": "rm -rf dist && node scripts/pre-dev.js",
    "dev": "webpack --mode development --env BROWSER=chrome   --watch",
    "predev:ff": "rm -rf dist-firefox && BROWSER=firefox node scripts/pre-dev.js",
    "dev:ff": "webpack --mode development --env BROWSER=firefox  --watch --output-path=dist-firefox",
    "test": "mocha --timeout 5000 --require ./test/babel-register.js 'test/unit/**/*.js'",
    "testa": "mocha --timeout 5000 --require ./test/babel-register.js 'test/unit/apiService.spec.js'",
    "testb": "mocha --timeout 5000 --require ./test/babel-register.js 'test/unit/dappService.spec.js'",
    "cspell": "cspell \"src/**\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@babel/preset-react": "7.28.5",
    "@babel/plugin-transform-runtime": "7.28.5",
    "@babel/runtime": "7.28.4",
    "@babel/register": "7.28.3",
    "babel-loader": "9.2.1",
    "babel-plugin-module-resolver": "5.0.0",
    "babel-plugin-import": "1.13.8",
    "copy-webpack-plugin": "13.0.1",
    "css-loader": "7.1.1",
    "mocha": "11.7.5",
    "path-browserify": "1.0.1",
    "process": "0.11.10",
    "sass": "1.95.1",
    "sass-loader": "16.0.6",
    "stream-browserify": "3.0.0",
    "style-loader": "4.0.0",
    "webpack": "5.91.0",
    "webpack-cli": "6.0.1",
    "jszip": "3.10.1",
    "deepmerge": "4.2.2",
    "sinon": "21.0.0",
    "proxyquire": "2.1.3",
    "util": "0.12.5",
    "chai":"6.2.1"
  },
  "resolutions": {
    "cross-spawn": "^7.0.5",
    "base-x": "^3.0.11"
  },
  "overrides": {
    "cross-spawn": "^7.0.5",
    "base-x": "^3.0.11"
  }
}
